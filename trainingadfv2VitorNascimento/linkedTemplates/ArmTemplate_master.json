{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "trainingadfv2VitorNascimento"
		},
		"InputBlop_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'InputBlop'"
		},
		"OutputSQL_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'OutputSQL'"
		},
		"new-blob_properties_Copy Data To SQL_parameters_fileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"new-blob_properties_Copy Data To SQL_parameters_tableName": {
			"type": "string",
			"defaultValue": "@triggerBody().tableName"
		},
		"new-blob_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f999f944-5fcc-426a-a89b-7b5c99dabb5c/resourceGroups/ProjetoEstagio/providers/Microsoft.Storage/storageAccounts/projetoestagio"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "trainingadfv2VitorNascimento_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"InputBlop_connectionString": {
						"value": "[parameters('InputBlop_connectionString')]"
					},
					"OutputSQL_connectionString": {
						"value": "[parameters('OutputSQL_connectionString')]"
					},
					"new-blob_properties_Copy Data To SQL_parameters_fileName": {
						"value": "[parameters('new-blob_properties_Copy Data To SQL_parameters_fileName')]"
					},
					"new-blob_properties_Copy Data To SQL_parameters_tableName": {
						"value": "[parameters('new-blob_properties_Copy Data To SQL_parameters_tableName')]"
					},
					"new-blob_properties_typeProperties_scope": {
						"value": "[parameters('new-blob_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}